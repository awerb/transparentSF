    "queries": {
        "Monthly": "SELECT date_trunc_ym(install_datetime) as install_month, nhood, streetname, district, project_location, tc_measure, units, fiscal_yr, program, current_status, next_step, COUNT(*) as item_count WHERE install_datetime >= '2022-09-01' GROUP BY install_month, nhood, streetname, district, project_location, tc_measure, units, fiscal_yr, program, current_status, next_step",
        "Yearly": "SELECT date_trunc_ym(install_datetime) as install_month, nhood, streetname, district, project_location, tc_measure, units, fiscal_yr, program, current_status, next_step, COUNT(*) as item_count WHERE install_datetime >= '2022-09-01' GROUP BY install_year, nhood, streetname, district, project_location, tc_measure, units, fiscal_yr, program, current_status, next_step"
