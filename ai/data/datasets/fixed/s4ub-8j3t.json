    "queries": {
        "Monthly": "SELECT date_trunc_ym(date) AS activity_month, count(*) AS total_activities, sum(amount) AS total_amount, type AS activity_type, lobbyisttype AS lobbyist_type WHERE date >= '2022-09-01' GROUP BY activity_month, activity_type, lobbyist_type",
        "Yearly": "SELECT date_trunc_y(date) AS year, count(*) AS total_activities, sum(amount) AS total_amount, type AS activity_type, lobbyisttype AS lobbyist_type WHERE date >= '2022-09-01' GROUP BY activity_year, activity_type, lobbyist_type"
    }
