    "queries": {
        "Monthly": "SELECT date_trunc_ym(start_date) AS start_date, date_trunc_ym(end_date) AS end_date, type, status, street, direction, veh_imp, COUNT(*) AS item_count WHERE start_date >= start_date GROUP BY start_date, end_date, type, status, street, direction, veh_imp",
