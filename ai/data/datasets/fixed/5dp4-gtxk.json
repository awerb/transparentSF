        "Yearly": "SELECT supervisor_district, boiler_type, status, date_trunc_y(application_date) AS year, count(*) AS permit_count WHERE application_date>='2022-09-01' GROUP BY supervisor_district, boiler_type, status, year ORDER BY year, supervisor_district, boiler_type, status"
