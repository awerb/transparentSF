    "queries": {
        "Monthly": "SELECT ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName, date_trunc_ym(ActionDate) as Month, COUNT(TreeID) as Number_of_Trees WHERE ActionDate >= '2022-09-01' GROUP BY ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName, Month ORDER BY Month, ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName",
        "Yearly": "SELECT ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName, date_trunc_ym(ActionDate) as Month, COUNT(TreeID) as Number_of_Trees WHERE ActionDate >= '2022-09-01' GROUP BY ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName, year ORDER BY Month, ActionType, LegalStatus, SiteType, PlantType, ScientificName, CommonName"
    }
